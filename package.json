{
    "name": "zoia2",
    "version": "1.0.0",
    "description": "Zoia 2 CMS",
    "main": "bin/server.js",
    "scripts": {
        "build": "webpack --mode production --config ./etc/webpack-frontend.config.js && webpack --mode production --config ./etc/webpack-api.config.js",
        "build-dev": "webpack --mode development --config ./etc/webpack-frontend.config.js && webpack --mode development --config ./etc/webpack-api.config.js",
        "build-frontend": "webpack --mode production --config ./etc/webpack-frontend.config.js",
        "build-frontend-dev": "webpack --mode development --config ./etc/webpack-frontend.config.js",
        "build-api": "webpack --mode production --config ./etc/webpack-api.config.js",
        "build-api-dev": "webpack --mode development --config ./etc/webpack-api.config.js",
        "start": "node ./bin/server.js",
        "add-locale-admin": "lingui add-locale --config ./etc/.linguirc-admin",
        "extract-admin": "lingui extract --config ./etc/.linguirc-admin",
        "compile-admin": "lingui compile --config ./etc/.linguirc-admin",
        "add-locale-user": "lingui add-locale --config ./etc/.linguirc-user",
        "extract-user": "lingui extract --config ./etc/.linguirc-user",
        "compile-user": "lingui compile --config ./etc/.linguirc-user",
        "rebuild-locales": "lingui compile --config ./etc/.linguirc-admin && lingui compile --config ./etc/.linguirc-user",
        "setup": "node ./bin/setup.js",
        "split-locales": "node ./bin/setup.js -s",
        "combine-locales": "node ./bin/setup.js -c"
    },
    "keywords": [
        "zoia2",
        "cms",
        "react",
        "uikit",
        "mongo",
        "fastify"
    ],
    "author": "Michael Matveev <xtreme@rh1.ru>",
    "license": "MIT",
    "dependencies": {
        "@babel/runtime": "^7.5.4",
        "@lingui/core": "^2.8.3",
        "@lingui/macro": "^2.8.3",
        "@lingui/react": "^2.8.3",
        "@types/react-router-dom": "^4.3.4",
        "ajv": "^6.10.2",
        "axios": "^0.19.0",
