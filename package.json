{
  "name": "zoia2",
  "version": "2.0.4",
  "description": "Zoia CMS",
  "main": "bin/api.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/xtremespb/zoia2"
  },
  "scripts": {
    "build": "webpack --display minimal --progress --mode production --config ./shared/build/webpack.config.js",
    "release": "webpack --display minimal --progress --mode production --config ./shared/build/webpack.config.js --do:release",
    "build-dev": "webpack --display minimal --progress --mode development --config ./shared/build/webpack.config.js",
    "build-admin": "webpack --display minimal --progress --mode production --config ./shared/build/webpack.config.js --build:admin",
    "build-admin-dev": "webpack --display minimal --progress --mode development --config ./shared/build/webpack.config.js --build:admin",
    "build-web": "webpack --display minimal --progress --mode production --config ./shared/build/webpack.config.js --build:web",
    "build-web-dev": "webpack --display minimal --progress --mode development --config ./shared/build/webpack.config.js --build:web",
    "build-api": "webpack --display minimal --progress --mode production --config ./shared/build/webpack.config.js --build:api",
    "build-api-dev": "webpack --display minimal --progress --mode development --config ./shared/build/webpack.config.js --build:api",
    "build-tools": "webpack --display minimal --progress --mode production --config ./shared/build/webpack.config.js --build:tools",
    "build-tools-dev": "webpack --display minimal --progress --mode development --config ./shared/build/webpack.config.js --build:tools",
    "start-api": "node ./bin/api.js",
    "start-web": "node ./bin/web.js",
    "add-locale": "lingui add-locale --config ./shared/build/linguirc.admin.json",
    "extract": "lingui extract --config ./shared/build/linguirc.admin.json",
    "compile": "lingui compile --config ./shared/build/linguirc.admin.json",
    "configure": "node ./bin/configure.js",
    "server-config": "node ./bin/server-config.js",
    "install-modules": "node ./bin/tools.js -i",
    "install-defaults": "node ./bin/tools.js -r",
    "split-locales": "node ./bin/tools.js -s",
    "combine-locales": "node ./bin/tools.js -c",
    "generate-package": "node ./bin/package.js"
  },
  "keywords": [
    "zoia2",
    "cms",
    "react",
    "uikit",
    "mongo",
    "fastify"
  ],
  "author": "Michael Matveev <xtreme@rh1.ru>",
